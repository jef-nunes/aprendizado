    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "comment",
                "settings": {
                    "foreground": "#4b4e5c"
                }
            },
            {
                "scope": ["constant.numeric","constant.language.python"],
                "settings": {
                    "foreground": "#2b922b",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "string",
                "settings": {
                    "foreground": "#25deff"
                }
            },
            {
                "scope": ["keyword.control","keyword.operator.logical.python"],
                "settings": {
                    "foreground": "#ff5f5f"
                }
            },
            {
                "scope": "keyword.operator",
                "settings": {
                    "foreground": "#b2ff88",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "storage.type",
                "settings": {
                    "foreground": "#676a6a",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "entity.name.function.python",
                "settings": {
                    "foreground": "#aef36d"
                }
            },
            {
                "scope": "storage.type.class.python",
                "settings": {
                    "foreground": "#225ae9",
                    "fontStyle": "italic bold"
                }
            },
            {
                "scope": ["entity.name.type.class.python"],
                "settings": {
                    "foreground": "#2a75ff",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "entity.name.function.decorator.python",
                "settings": {
                    "foreground": "#96a4a8"
                }
            },
            {
                "scope": "source.python",
                "settings": {
                    "foreground": "#00ff2a"
                }
            },
            {
                "scope": "variable.parameter.function.language.python",
                "settings": {
                    "foreground": "#669455"
                }
            }
        ]
    }
